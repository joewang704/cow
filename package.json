{
  "name": "shortweb-app",
  "version": "1.0.0-alpha.1",
  "private": true,
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "express": "^4.13.4",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.13.0",
    "randomcolor": "^0.4.4",
    "react": "^0.14.7",
    "react-bootstrap-switch": "^3.4.4",
    "react-dom": "^0.14.7",
    "react-notification": "^5.0.5",
    "react-popover": "^0.4.4",
    "react-radio-group": "^2.2.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "request-promise": "^3.0.0",
    "transit-immutable-js": "^0.6.0",
    "transit-js": "^0.8.846"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "0.0.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "npm-watch": "^0.1.4",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.13.1"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,scss",
      "ignore": [
        "node_modules",
        "src/static",
        "src/server"
      ]
    },
    "dev": {
      "patterns": [
        "src/static",
        "src/server"
      ]
    }
  },
  "scripts": {
    "start": "node src/server/index.js",
    "build": "webpack",
    "dev": "babel-node src/server/index.js",
    "test": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- test/**/*",
    "test:watch": "npm test -- --watch",
    "watch": "npm-watch"
  }
}
