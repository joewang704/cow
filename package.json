{
  "name": "shortweb-app",
  "version": "1.0.0-alpha.1",
  "private": true,
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "express": "^4.13.4",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.13.0",
    "randomcolor": "^0.4.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-notification": "^5.0.5",
    "react-popover": "^0.4.4",
    "react-radio-group": "^2.2.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "request-promise": "^3.0.0",
    "transit-immutable-js": "^0.6.0",
    "transit-js": "^0.8.846",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "^0.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "npm-watch": "^0.1.4",
    "react-hot-loader": "^3.0.0-beta.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,scss",
      "ignore": [
        "node_modules",
        "src/static",
        "src/server"
      ]
    },
    "dev": {
      "patterns": [
        "src/static",
        "src/server"
      ]
    }
  },
  "scripts": {
    "start": "node src/server/index.js",
    "build": "webpack",
    "dev": "babel-node src/server/index.js",
    "test": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- test/**/*",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .eslintrc src || true",
    "watch": "npm-watch"
  }
}
