{
  "name": "shortweb-app",
  "version": "1.0.0-alpha.1",
  "private": true,
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "express": "^4.13.4",
    "immutable": "^3.7.6",
    "randomcolor": "^0.4.4",
    "react": "^0.14.7",
    "react-bootstrap-switch": "^3.4.4",
    "react-dom": "^0.14.7",
    "react-popover": "^0.4.4",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "0.0.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,scss",
      "ignore": [
        "node_modules",
        "src/static"
      ]
    }
  },
  "scripts": {
    "start": "node src/server.js",
    "build": "webpack",
    "dev": "nodemon --exec babel-node src/server.js",
    "clean": "rm -rf build",
    "test": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- test/**/*",
    "test:watch": "npm test -- --watch",
    "watch": "npm-watch"
  }
}
