{
  "extends": "airbnb",
  "rules": {
    "semi": [2, "never"],
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "no-use-before-define": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "radix": 0,
    "react/prop-types": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-no-bind": 0
  }
}
